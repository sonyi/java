create database library;
use library;

create table usersys(
		uID int not null auto_increment primary key,
		uName varchar(50) not null,
		upassword varchar(20) not null	
	);
	
insert into usersys(uname,upassword) values('zhangsan','12345');
insert into usersys(uname,upassword) values('lisi','123');
insert into usersys(uname,upassword) values('admin','admin');
insert into usersys(uname,upassword) values('test','123456');



create table booksData(
		bID int not null primary key,
		bName varchar(50) not null,
		bAuth varchar(30) not null,
		bsum int default 0
	);
	
insert into booksData(bid,bname,bauth,bsum) values(5,'蒋介石与现代中国','陶涵',12);
